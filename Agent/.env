# Agent服务环境变量配置文件
# 此文件包含所有必需的配置值，请根据实际情况填写

# 阿里云DashScope配置
DASHSCOPE_API_KEY=your_dashscope_api_key_here
DASHSCOPE_MODEL=qwen-plus-0806

# 天气API配置 - 使用Open-Meteo免费服务
WEATHER_API_KEY=
WEATHER_API_URL=https://api.open-meteo.com/v1

# 百度地图API配置
BAIDU_MAP_AK=your_baidu_map_ak_here
BAIDU_MAP_API_URL=https://api.map.baidu.com

# 数据库配置
DATABASE_HOST=localhost
DATABASE_PORT=3306
DATABASE_NAME=heritage_db
DATABASE_USER=root
DATABASE_PASSWORD=your_database_password_here

# 后端API配置
BACKEND_API_URL=http://localhost:8000/api

# 日志配置
LOG_LEVEL=WARNING
LOG_FILE_PATH=logs/agent.log

# 生产环境配置
DEVELOPMENT=false
DEBUG=false
